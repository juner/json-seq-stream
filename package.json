{
  "name": "json-seq-stream",
  "version": "1.0.2",
  "description": "application/json-seq support stream / async iterator library",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.umd.cjs"
    }
  },
  "types": "./index.d.ts",
  "scripts": {
    "test:watch": "vitest watch --coverage",
    "test:run": "vitest run --coverage --coverage.reporter=cobertura --coverage.reporter=text",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "vite build && tsc --project tsconfig.build.json && cpx ./package.json dist && cpx README.md dist && cpx LICENSE dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juner/json-seq-stream.git"
  },
  "keywords": [
    "application/json-seq",
    "TransformStream",
    "web streams",
    "asyncIterator",
    "Symbol.asyncIterator",
    "rfc7464",
    "json-seq"
  ],
  "author": "juners",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juner/json-seq-stream/issues"
  },
  "homepage": "https://github.com/juner/json-seq-stream#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cpx": "^1.5.0",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "path": "^0.12.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.3",
    "vitest": "^3.2.4"
  }
}
